docker {
       enabled = true
       temp = 'auto'
}

profiles {
         standard {
                  process.executor      = 'local'
         }

         cluster {
                 process {
                         cpus = 24
                         executor {
                                  name          = 'sge'
                                  pollInterval  = '10sec'
                                  queueSize     = 100
                         }
                 }
         }

}
