docker {
       enabled = true
       temp = 'auto'
}

profiles {
         standard {
                  process.executor      = 'local'
         }

         cluster {
                 process {
                         executor = 'sge'
                         queue    = 'short-sl7'
                 }
         }
}

executor {
         name          = 'sge'
         pollInterval  = '10sec'
         queueSize     = 100
}
